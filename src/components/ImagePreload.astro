<script>
    const preloadImages = () => {
      const imagesToPreload = [
        'https://picsum.photos/seed/manga-cover/600/900'
      ];
  
      imagesToPreload.forEach(src => {
        const img = new Image();
        img.src = src;
      });
    };
  
    // Preload images after the main content has loaded
    window.addEventListener('load', preloadImages);
  </script>